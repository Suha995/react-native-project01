+                perspective: 1000
+            },
+            {
+                scale: animatedValue.interpolate({
+                    inputRange: [-1, 0, 1],
+                    outputRange: [0.65, 1, 0.65],
+                    extrapolate: 'clamp'
+                })
+            },
+            {
+                rotateX: animatedValue.interpolate({
+                    inputRange: [-1, 0, 1],
+                    outputRange: ['30deg', '0deg', '30deg'],
+                    extrapolate: 'clamp'
+                })
+            },
+            {
+                rotateY: animatedValue.interpolate({
+                    inputRange: [-1, 0, 1],
+                    outputRange: ['-30deg', '0deg', '30deg'],
+                    extrapolate: 'clamp'
+                })
+            }
+        ]
+    };
+}
+
+// Exports
